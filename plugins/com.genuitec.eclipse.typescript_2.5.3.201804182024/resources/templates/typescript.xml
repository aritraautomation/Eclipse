<?xml version="1.0" encoding="UTF-8"?>
<templates>

<template name="for" description="%Templates.for_array" id="com.genuitec.eclipse.typescript.templates.for_array" context="typescript" enabled="true" autoinsert="false">for (let ${index} = 0; ${index} &lt; ${array}.length; ${index}++) {
	${line_selection}${cursor}
}</template>

<template name="for" description="%Templates.for_temp" id="com.genuitec.eclipse.typescript.templates.for_temp" context="typescript" enabled="true" autoinsert="false">for (let ${index} = 0; ${index} &lt; ${array}.length; ${index}++) {
	let ${array_element} = ${array}[${index}];
	${cursor}
}</template>

<template name="forin" description="%Templates.for_iterable" id="com.genuitec.eclipse.typescript.templates.for_iterable" context="typescript" enabled="true" autoinsert="false">for (let ${iterable_element} in ${iterable}) {
