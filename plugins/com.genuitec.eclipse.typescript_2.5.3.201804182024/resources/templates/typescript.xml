} while (${condition});</template>

<template name="switch" description="%Templates.switch" id="com.genuitec.eclipse.typescript.templates.switch" context="typescript" enabled="true" autoinsert="false">switch (${key}) {
	case ${value}:
		${cursor}
		break;
	default:
		break;
}</template>

